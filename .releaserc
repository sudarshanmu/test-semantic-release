{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    {name: "saiteja", prerelease: true},
    {name: "beta", prerelease: true},
    {name: "alpha", prerelease: true}
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits"
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "presetConfig": {
          "issueUrlFormat": "https://claimyourmerit.atlassian.net/browse/{{prefix}}{{id}}",
          "issuePrefixes": [
            "GS-"
          ]
        },
        "preset": "conventionalcommits"
      }
    ],
    [
      "@semantic-release/exec", {
        "successCmd": './shell.sh'
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["app.json", "CHANGELOG.md", "package.json", "package-lock.json", "node_modules/**/"],
        "message": "chore(release): ${nextRelease.version} (${nextRelease.type}) - <%= new Date().toLocaleDateString('en-US', {year: 'numeric', month: 'short', day: 'numeric', hour: 'numeric', minute: 'numeric' }) %> [skip ci]\n\nFrom ${lastRelease.version} to ${nextRelease.version}\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ]
}
